<title>@ViewBag.CurrentApplicationMerchantApp.MerchantAppName</title>
<meta name="viewport" content="width=device-width" />
<script type="text/javascript">
    (function (config) {
        config.RootUrl = "@XCLNetTools.StringHander.Common.RootUri";
        config.AllowUploadExtInfo= "@(ViewBag.AllowUploadExtInfo)";
    })(window.AppConfig = window.AppConfig || {});
</script>

<link href="~/Resources/dist/style.css?v=@(ViewBag.ResourceVersion)" rel="stylesheet" />
<script src="~/Resources/dist/plugins.js?v=@(ViewBag.ResourceVersion)"></script>
<script src="~/Resources/dist/app.js?v=@(ViewBag.ResourceVersion)"></script>
<script type="text/javascript">
    (function (go) {
        go.globalSettings({
            templateName: "artdialog",
            isExclusive:false
        });
    })($.XGoAjax);
    (function (config) {
        config.lock = true;
        config.opacity = 0.2;
        config.resize = true;
        config.path="@Url.Content("~/Resources/src/js/artDialog")";
    })(art.dialog.defaults);
</script>